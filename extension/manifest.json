{
    "manifest_version": 3,
    "name": "Vulture",
    "description": "Page Indexer and To do list",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/parser.js"
            ]
        }
    ],
	"omnibox": { "keyword" : "v" },
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "storage",
        "unlimitedStorage",
        "notifications",
        "alarms"
    ],
    "action": {
        "default_icon": "assets/placeholder.png",
        "default_title": "Click Me.",
        "default_popup": "assets/popup.html"
    },
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    }
}